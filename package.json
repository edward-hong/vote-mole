{
	"name": "vote-mole",
	"version": "1.0.0",
	"description": "Voting App",
	"repository": {
		"type": "git",
		"url": "git://github.com/edward-hong/vote-mole.git"
	},
	"main": "index.js",
	"engines": {
		"node": "10.x",
		"yarn": "1.x"
	},
	"scripts": {
		"start": "node server/index.js",
		"server": "nodemon --watch server server/index.js",
		"client": "yarn --cwd client start",
		"dev": "concurrently --names \"server,client\" \"yarn server --silent\" \"yarn client --silent\"",
		"build": "yarn --cwd client && yarn --cwd client build",
		"deploy": "git push heroku master"
	},
	"keywords": [
		"MongoDB",
		"Express",
		"React",
		"Node",
		"MERN"
	],
	"author": "Edward Hong",
	"license": "ISC",
	"dependencies": {
		"bootstrap": "^4.3.1",
		"concurrently": "^4.1.0",
		"connect-mongo": "^2.0.3",
		"dotenv": "^8.0.0",
		"express": "^4.16.4",
		"express-session": "^1.16.1",
		"mongoose": "^5.5.6",
		"passport": "^0.4.0",
		"passport-facebook": "^3.0.0",
		"passport-github": "^1.1.0",
		"passport-google-oauth20": "^2.0.0",
		"passport-twitter": "^1.0.4",
		"promise-redis": "^0.0.5",
		"ramda": "^0.26.1",
		"react-router-dom": "^5.0.0",
		"reactstrap": "^8.0.0",
		"redis": "^2.8.0",
		"request-ip": "^2.1.3",
		"styled-components": "^4.2.0"
	},
	"devDependencies": {
		"nodemon": "^1.19.0"
	}
}
